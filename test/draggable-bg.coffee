describe 'Angular Draggable BG', ->

  el = scope = $document = $rootScope = $compile = null
  imgData = 'data:image/jpeg;base64,'
  imgSize = w: 700, h: 467
  divSize = w: 200, h: 200
  initialImgPosition = null

  beforeEach ->
    module 'chieffancypants.draggableBg'

    inject (_$rootScope_, _$compile_, _$document_) ->
      $compile = _$compile_
      $document = _$document_
      $rootScope = _$rootScope_
      scope = $rootScope.$new()
      scope.image = {url: imgData}

  describe 'background-size: auto', ->

    beforeEach (done) ->
      el = $compile('<div id="test-el" style="width:' + divSize.w + 'px; height: ' + divSize.h + 'px" draggable-bg="image.url"></div>')(scope)
      document.body.appendChild(el[0])
      scope.$digest()
      $rootScope.$on 'draggableBg:ready', (e, args) ->
        initialImgPosition = args
        done()

    afterEach ->
      angular.element(document.getElementById('test-el')).remove()

    it 'should center the image by default', ->
      expect(initialImgPosition).toEqual
        x: 50
        y: 50

    it 'should position the image as the user drags', ->
      eventArgs = null
      $rootScope.$on 'draggableBg:repositioned', (event, args) -> eventArgs = args

      el.triggerHandler
        type: 'mousedown', pageX: 0, pageY: 0
      $document.triggerHandler
        type: 'mousemove', pageX: -20, pageY: -35
      $document.triggerHandler 'mouseup'

      scope.$digest()

      expect(eventArgs).toEqual
        x: 54
        y: 63.10861423220974

    it "should do nothing if the mouse doesn't move", ->
      eventArgs = null
      $rootScope.$on 'draggableBg:repositioned', (event, args) -> eventArgs = args

      el.triggerHandler
        type: 'mousedown', pageX: 0, pageY: 0
      $document.triggerHandler
        type: 'mousemove', pageX: 0, pageY: 0
      $document.triggerHandler 'mouseup'

      scope.$digest()
      expect(eventArgs).toEqual initialImgPosition

    it 'should constrain the image from moving past its bounds', ->
      eventArgs = null
      $rootScope.$on 'draggableBg:repositioned', (event, args) -> eventArgs = args

      # Move the image to its top left corner
      el.triggerHandler
        type: 'mousedown', pageX: 0, pageY: 0
      $document.triggerHandler
        type: 'mousemove', pageX: 2000, pageY: 3000
      $document.triggerHandler 'mouseup'
      scope.$digest()
      expect(eventArgs).toEqual x: 0, y: 0

      # Move the image to its bottom right corner
      el.triggerHandler
        type: 'mousedown', pageX: 0, pageY: 0
      $document.triggerHandler
        type: 'mousemove', pageX: -2000, pageY: -3000
      $document.triggerHandler 'mouseup'
      scope.$digest()
      expect(eventArgs).toEqual
        x: 100
        y: 100

  describe 'background-size: cover', ->

    beforeEach (done) ->
      el = $compile('<div id="test-el" style="width:' + divSize.w + 'px; height: ' + divSize.h + 'px; background-size: cover" draggable-bg="image.url"></div>')(scope)
      document.body.appendChild(el[0])
      scope.$digest()
      $rootScope.$on 'draggableBg:ready', (e, args) ->
        initialImgPosition = args
        done()

    afterEach ->
      angular.element(document.getElementById('test-el')).remove()

    it 'should position the image as the user drags', ->
      eventArgs = null
      $rootScope.$on 'draggableBg:repositioned', (event, args) -> eventArgs = args

      el.triggerHandler
        type: 'mousedown', pageX: 0, pageY: 0
      $document.triggerHandler
        type: 'mousemove', pageX: -20, pageY: -30
      $document.triggerHandler 'mouseup'

      scope.$digest()

      expect(eventArgs).toEqual
        x: 70.04291845493562
        y: 100
